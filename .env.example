# Gemini API key used by the app (recommended for Vite client-side access)
VITE_API_KEY=your_gemini_api_key

# Optional: also set this for tooling that expects GEMINI_API_KEY
GEMINI_API_KEY=your_gemini_api_key

# Supabase project URL and anon key used for authentication
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
